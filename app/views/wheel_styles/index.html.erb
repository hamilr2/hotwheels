<h1>Listing wheel_styles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wheel_styles.each do |wheel_style| %>
  <tr>
    <td><%= wheel_style.name %></td>
    <td><%= wheel_style.desc %></td>
    <td><%= link_to 'Show', wheel_style %></td>
    <td><%= link_to 'Edit', edit_wheel_style_path(wheel_style) %></td>
    <td><%= link_to 'Destroy', wheel_style, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

